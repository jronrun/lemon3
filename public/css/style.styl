ul
  list-style-type: none

.borderno
  border: 0

//http://stackoverflow.com/questions/18996202/twitter-bootstrap-button-text-word-wrap
.linebreak{ white-space:normal !important; word-wrap:break-word; }

.roundedcorner { border-radius: 0.3rem; }
.text-silver { color: silver; }

.boxshadow { -webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175); box-shadow: 0 6px 12px rgba(0, 0,0,.175); }

//<div class="icondh"><em class="fa fa-question-circle"></em></div>
.icondh { cursor: pointer; }
.icondh em { vertical-align: middle; margin: 0; display: inline-block; text-align: center; -webkit-transition: all 1s; transition: all 1s; }
.icondh:hover em { -webkit-transform: scale(2, 2); transform: scale(2, 2); }

.subform { border: 1px solid #d9edf7; padding: 10px; }

.logo { display: inline-block; width: 36px; height: 16px; vertical-align: middle; margin-right: 2px; margin-left: -11px; position: relative; }
.logo .l { width: 11px; height: 11px; border-radius: 50%; background-color: #52BCD3; position: absolute; }
.logo .l.l1 { bottom: 0; left: 0; }
.logo .l.l2 { width: 10px; height: 7px; bottom: 13px; left: 5px; }
.logo .l.l3 { width: 5px; height: 5px; bottom: 2px; left: 0px; }
.logo .l.l4 { height: 16px; width: 8px; bottom: 8px; left: 20px; }
.logo .l.l5 { width: 18px; bottom: 0; left: 29px; }

.cm-s-lemon .CodeMirror { background-color:#ffffff; color:#2e383c; line-height:1.4375; }
.cm-s-lemon .cm-comment {color:silver}
.cm-s-lemon .cm-keyword, .cm-s-lemon .cm-property {color:gray}
.cm-s-lemon .cm-atom {color: magenta} .cm-s-lemon .cm-number {color:blue}
.cm-s-lemon .cm-node,.cm-s-lemon .cm-tag {color:#9c3328}
.cm-s-lemon .cm-string {color:green}
.cm-s-lemon .cm-variable,.cm-s-lemon .cm-qualifier {color:#047d65}
.cm-s-lemon pre { padding:0; }
.cm-s-lemon .CodeMirror-gutters { border:none; border-right:10px solid transparent; background-color:transparent; }
.cm-s-lemon .CodeMirror-linenumber { padding:0; color:#e0e2e5; }
.cm-s-lemon .CodeMirror-guttermarker { color: #1d75b3; }
.cm-s-lemon .CodeMirror-guttermarker-subtle { color: #e0e2e5; }


div.console { font-size: 14px; margin-top:1em }
div.console div.jquery-console-inner { background:white; padding:0.5em; overflow:auto }
div.console div.jquery-console-prompt-box { color:grey; font-family:monospace; }
div.console div.jquery-console-focus span.jquery-console-cursor { background: silver; color:#333; font-weight:bold }
div.console div.jquery-console-message-error { color:#ef0505; font-family:sans-serif; font-weight:bold; padding:0.1em; }
div.console div.jquery-console-message-success { color:#187718; font-family:monospace; padding:0.1em; }
div.console div.jquery-console-message-value { color:#1ad027; font-family:monospace; padding:0.1em; }
div.console div.jquery-console-message-type { color:#52666f; font-family:monospace; padding:0.1em; }
div.console span.jquery-console-prompt-label { color: #4b7980; font-weight:bold }
